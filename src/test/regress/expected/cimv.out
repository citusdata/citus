CREATE MATERIALIZED VIEW mv WITH (citus.cimv) AS
SELECT a,
       date_trunc('hour', d) AS d_hour,
       min(b) AS min_b,
       max(b) AS max_b,
       avg(b) AS avg_b,
       min(c) AS min_c,
       max(c) AS max_c,
       avg(c) AS avg_c,
       min(e) AS min_e,
       max(e) AS max_e,
       avg(e) AS avg_e
