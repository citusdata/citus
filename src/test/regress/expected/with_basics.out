WITH distinct_undistribured AS (
    SELECT DISTINCT user_id
    FROM test_cte
)
SELECT count(*)
FROM distinct_undistribured
LEFT JOIN
  (SELECT *,
          random()
   FROM test_cte_distributed d1
   WHERE NOT EXISTS
       (SELECT NULL
        FROM distinct_undistribured d2
        WHERE d1.user_id = d2.user_id )) AS bar USING (user_id);
