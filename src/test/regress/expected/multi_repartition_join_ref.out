SET citus.log_multi_join_order to TRUE;
SET client_min_messages to DEBUG1;
SET citus.enable_repartition_joins to on;
SELECT
    l_partkey, l_suppkey, count(*)
FROM
    lineitem, part_append, supplier
WHERE
    l_partkey = p_partkey
    AND l_suppkey < s_suppkey
GROUP BY
    l_partkey, l_suppkey
ORDER BY
    l_partkey, l_suppkey
LIMIT 10;
