---------------------------------------------------------------------
 key-1 | value-2 |     1
(1 row)

-- left join with non-pushdownable window functions
SELECT a.key, a.value, count(a.value) OVER ()
FROM items a LEFT JOIN other_items b ON (a.key = b.key)
